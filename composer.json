{
    "name": "morris-php/youtube-api",
    "description": "Exposes endpoints for retrieving youtube videos and channels and working with the data.",
    "keywords": ["youtube", "rest", "psr7"],
    "authors": [
        {
            "name": "Ben Morris",
            "email": "benjamin.morris77@gmail.com"
        }
    ],
    "require": {
        "ext-pdo": "*",
        "ext-json": "*",
        "google/apiclient": "^2.2",
        "monolog/monolog": "^1.17",
        "php": ">=7.1",
        "slim/php-view": "^2.0",
        "slim/slim": "^3.1",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "MorrisPhp\\YouTubeApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
