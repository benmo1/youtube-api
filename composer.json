{
    "name": "morris-php/play-sports-backend-task",
    "description": "Exposes endpoints for retrieving youtube videos and channels and working with the data.",
    "keywords": ["youtube", "rest", "psr7"],
    "authors": [
        {
            "name": "Ben Morris",
            "email": "benjamin.morris77@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "monolog/monolog": "^1.17",
        "slim/php-view": "^2.0",
        "slim/slim": "^3.1",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "MorrisPhp\\YouTubeApp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
